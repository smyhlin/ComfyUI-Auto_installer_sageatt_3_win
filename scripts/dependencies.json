{
  "tools": {
    "python": {
      "version": "3.13.7",
      "url": "https://www.python.org/ftp/python/3.13.7/python-3.13.7-amd64.exe",
      "arguments": "/quiet InstallAllUsers=1 PrependPath=1"
    },
    "git": {
      "version": "2.51.0.2",
      "url": "https://github.com/git-for-windows/git/releases/download/v2.51.0.windows.2/Git-2.51.0.2-64-bit.exe",
      "arguments": "/VERYSILENT"
    },
    "seven_zip": {
      "name": "7-Zip",
      "version": "25.01",
      "url": "https://www.7-zip.org/a/7z2501-x64.exe",
      "install_path": "C:\\Program Files\\7-Zip"
    },
    "aria2": {
      "name": "aria2",
      "version": "1.37.0",
      "url": "https://github.com/aria2/aria2/releases/download/release-1.37.0/aria2-1.37.0-win-64bit-build1.zip",
      "install_path": "C:\\ProgramData\\aria2"
    },
    "ninja": {
      "name": "ninja",
      "version": "1.13.1",
      "url": "https://github.com/ninja-build/ninja/releases/download/v1.13.1/ninja-win.zip",
      "install_path": "C:\\ProgramData\\ninja"
    },
    "vs_build_tools": {
      "url": "https://aka.ms/vs/17/release/vs_BuildTools.exe",
      "arguments": "--quiet --wait --norestart --add Microsoft.VisualStudio.Component.VC.Tools.x86.x64 --add Microsoft.VisualStudio.Component.VC.CMake.Project --add Microsoft.VisualStudio.Component.Windows11SDK.26100",
      "install_path": "C:\\Program Files (x86)\\Microsoft Visual Studio\\2022\\BuildTools"
    },
    "ccache": {
      "name": "ccache",
      "version": "4.12.1",
      "url": "https://github.com/ccache/ccache/releases/download/v4.12.1/ccache-4.12.1-windows-x86_64.zip",
      "install_path": "C:\\ProgramData\\ccache"
    }
  },
  "repositories": {
    "comfyui": {
        "url": "https://github.com/comfyanonymous/ComfyUI.git"
    },
    "workflows": {
        "url": "https://github.com/UmeAiRT/ComfyUI-Workflows"
    }
  },
  "pip_packages": {
    "upgrade": [
      "pip",
      "wheel"
    ],
    "torch": {
      "packages": "--pre torch torchvision torchaudio",
      "index_url": "https://download.pytorch.org/whl/nightly/cu129"
    },
    "comfyui_requirements": "requirements.txt",
    "wheels": [
      {
        "name": "nunchaku-1.0.1+torch2.8-cp313-cp313-win_amd64.whl",
        "url": "https://github.com/nunchaku-tech/nunchaku/releases/download/v1.0.1/nunchaku-1.0.1+torch2.8-cp313-cp313-win_amd64.whl"
      },
      {
        "name": "insightface-0.7.3-cp313-cp313-win_amd64",
        "url": "https://github.com/Gourieff/Assets/blob/main/Insightface/insightface-0.7.3-cp313-cp313-win_amd64.whl"
      },
      {
        "name": "triton_windows-3.4.0.post20-cp313-cp313-win_amd64",
        "url": "https://files.pythonhosted.org/packages/a2/cc/5bcad4a71bcab57f9b1c95fe20b91bd294b86f988007072a6e01fa3f9591/triton_windows-3.4.0.post20-cp313-cp313-win_amd64.whl"
      }
    ],
    "standard": [
      "facexlib",
      "cython",
      "onnxruntime",
      "onnxruntime-gpu",
      "triton-windows"
    ],
    "pinned": [
      "numpy==1.26.4",
      "pandas",
      "transformers==4.49.0"
    ],
    "git_repos": [
      {
        "name": "xformers",
        "url": "https://github.com/facebookresearch/xformers.git",
        "commit": "main"
      },
      {
        "name": "SageAttention",
        "url": "https://github.com/thu-ml/SageAttention",
        "commit": "main"
      },
      {
        "name": "apex",
        "url": "https://github.com/NVIDIA/apex.git",
        "commit": "master",
        "install_options": "-v --disable-pip-version-check --no-cache-dir --no-build-isolation --config-settings \"--build-option=--cpp_ext\" --config-settings \"--build-option=--cuda_ext\""
      }
    ]
  },
  "files": {
    "comfy_settings": {
      "url": "https://huggingface.co/UmeAiRT/ComfyUI-Auto_installer/resolve/main/others/comfy.settings.json",
      "destination": "user/default/comfy.settings.json"
    },
    "custom_nodes_csv": {
        "url": "https://github.com/UmeAiRT/ComfyUI-Auto_installer/raw/main/scripts/custom_nodes.csv",
        "destination": "scripts/custom_nodes.csv"
    }
  }
}
